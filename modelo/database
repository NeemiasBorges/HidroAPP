CREATE database hidroapp;

use hidroapp;

CREATE TABLE user(
id INT AUTO_INCREMENT NOT NULL,
email VARCHAR(60),
local VARCHAR(60),
agua VARCHAR(60),
quant VARCHAR(60),
telefone VARCHAR(16),
PRIMARY KEY(id)
) engine = innodb;


